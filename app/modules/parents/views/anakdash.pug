		script.
			$(document).on('click', '#logOut', function(){
				window.location.href = '/parents/login'

			})
			$(document).on('click', '.acceptQuest', function(){
				var rewardName = $(this).parents('li.swipeout').find('#rewardName').text();
				console.log(rewardName)
				$('#rewardTitle').text(rewardName);
			})
			$(document).on('click', '.addMore', function(){
				var row = $(`<li><div class="feat_small_icon"><img src="/assets/images/icons/black/trophy.png" alt="" title="" /></div><div class="feat_small_details"><label>Gatimpala:</label><textarea class="form_textarea" name="message" rows="" cols=""></textarea></div></li>`)
				$('#gantimpalaField').append(row)
			})
			$(document).on('click', '.minusLess', function(){
				$('#gantimpalaField').find('li')[0].remove();
			})
			$(document).on('click', '#submitKahilingan', function(){
				var kahilinganInput = $('#kahilinganInput').val();
				$.post('/parents/anakkahilingan', {
					kahilinganInput: kahilinganInput,
				}).then(function(data){
					location.reload();
				})
			})
